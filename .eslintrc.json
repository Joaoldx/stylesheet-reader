{
  "env": {
    "browser": true,
    "es2021": true,
    "commonjs": true
  },
  "plugins": ["prettier"],
  "extends": ["prettier", "eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "commonjs"
  },
  "rules": {
    "no-console": "warn",
    "prettier/prettier": "error"
  }
}
